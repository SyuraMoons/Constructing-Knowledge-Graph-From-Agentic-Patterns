:MultiAgentSystemShape a sh:NodeShape ;
    sh:targetClass agento:MultiAgentSystem ;
    sh:property [
        sh:path agento:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] .

:LLMModelShape a sh:NodeShape ;
    sh:targetClass agento:LLMModel ;
    sh:property [
        sh:path agento:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] .
