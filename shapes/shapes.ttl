@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AgentShape a sh:NodeShape ;
    sh:targetClass agento:Agent ;
    sh:property [
        sh:path agento:name ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path agento:vendorClass ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:systemMessage ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path agento:humanInputMode ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path agento:role ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path agento:instructions ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path agento:modelName ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] .

:DatatypePropertyShape a sh:NodeShape ;
    sh:targetClass agento:DatatypeProperty ;
    sh:property [
        sh:path agento:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:domain ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:range ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:justification ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] .

:OptionalPropertyShape a sh:NodeShape ;
    sh:targetClass agento:OptionalProperty ;
    sh:property [
        sh:path agento:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:domain ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:range ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:justification ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] .

:OntologyRelationalPropertyShape a sh:NodeShape ;
    sh:targetClass agento:OntologyRelationalProperty ;
    sh:property [
        sh:path agento:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:domain ;
        sh:minCount 1 ;
    ] ;
    sh:property [
        sh:path agento:range ;
