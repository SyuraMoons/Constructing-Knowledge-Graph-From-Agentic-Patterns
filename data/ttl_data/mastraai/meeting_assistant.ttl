ex:meeting_assistant.json
    a :System ;
    dcterms:title "meeting_assistant.json" ;
    dcterms:description Pattern ini mendefinisikan sistem “Meeting Assistant” dengan dua agent: scheduler dan note_taker. Keduanya memiliki peran dan instruksi yang berbeda, namun sama-sama menggunakan model LLM gpt-4 untuk membantu penjadwalan pertemuan dan pencatatan notulen .


ex:llm_gpt-4
    a :LanguageModel ;
    dcterms:title "gpt-4" .


ex:DatatypeProperty_name
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Field name digunakan konsisten pada sistem, agent, dan model." ;
    agento:name "name" ;
    agento:range xsd:string .


ex:DatatypeProperty_role
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Membedakan fungsi Meeting Scheduler dan Note Taker dalam sistem." ;
    agento:name "role" ;
    agento:range xsd:string .


ex:DatatypeProperty_instructions
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Menjelaskan fungsi operasional masing-masing agent." ;
    agento:name "instructions" ;
    agento:range xsd:string .


ex:DatatypeProperty_modelname
    a agento:DatatypeProperty ;
    agento:domain agento:Agent ;
    agento:justification "Menyimpan “gpt-4” sebagai identitas model yang digunakan." ;
    agento:name "modelName" ;
    agento:range xsd:string .


ex:multiagentsystem
    a owl:Class ;
    dcterms:title "MultiAgentSystem" ;
    dcterms:description "Sistem top-level yang menaungi beberapa agent dalam domain meeting." .


ex:llmmodel
    a owl:Class ;
    dcterms:title "LLMModel" ;
    dcterms:description "Representasi model LLM yang digunakan oleh agent untuk reasoning." .
