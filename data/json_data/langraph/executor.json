{
    "framework": "Langraph",
    "file_name": "executor.py",
    "pattern_type": "Single-nodeworkflow",
    "description": "Pattern ini mendefinisikan sebuah StateGraph dengan satu node bernama executor_node. Node tersebut dijadikan entry point sekaligus finish point. Tidak ada node lain maupun edge tambahan. Struktur ini merepresentasikan workflow minimal di LangGraph",
    "entities": [
        {
            "id": "workflow",
            "vendorClass": "StateGraph()",
            "mapsTo": "agento:Agent",
            "attributes": {
                "-": "-"
            },
            "relations": {}
        },
        {
            "id": "node",
            "vendorClass": "add_node(name, fn)",
            "mapsTo": "agento:Agent",
            "attributes": {
                "nama": "executor_node",
                "callable": "lambda x: x"
            },
            "relations": {}
        },
        {
            "id": "entry point",
            "vendorClass": "set_entry_point(name)",
            "mapsTo": "agento:Agent",
            "attributes": {
                "name": "executor_node"
            },
            "relations": {}
        },
        {
            "id": "finish point",
            "vendorClass": "et_finish_point(name)",
            "mapsTo": "agento:Agent",
            "attributes": {
                "name": "executorr_node"
            },
            "relations": {}
        }
    ],
    "ontologyRelationalProperties": [
        {
            "name": "callsNode",
            "domain": "agento:Workflow",
            "range": "agento:Node",
            "definition": "Workflow memiliki node eksekusi",
