    "entities": [
        {
            "id": "recruitmentcrew",
            "vendorClass": "Crew",
            "mapsTo": "agento:Agent",
            "attributes": {
                "agents": "sourcing_specialist",
                "tasks": "resume_analyzer"
            },
            "relations": {}
        },
        {
            "id": "sourcing specialist",
            "vendorClass": "Agent",
            "mapsTo": "agento:Agent",
            "attributes": {
                "role": "Sourcing Specialist",
                "goal": "Find qualified candidates",
                "tools": "LinkedInScraperTool"
            },
            "relations": {}
        },
        {
            "id": "resume analyzer",
            "vendorClass": "Agent",
            "mapsTo": "agento:Agent",
            "attributes": {
                "role": "Resume Analyzer",
                "goal": "Screen and rank candidates",
                "tools": "CVParserTool"
            },
            "relations": {}
        },
        {
            "id": "interview coordinator",
            "vendorClass": "Agent",
            "mapsTo": "agento:Agent",
            "attributes": {
                "role": "Interview Coordinator",
                "goal": "Schedule interviews with qualified candidates",
                "tools": "CalendarTool"
            },
            "relations": {}
        },
        {
            "id": "sourcing task",
            "vendorClass": "Task",
            "mapsTo": "agento:Agent",
            "attributes": {
                "description": "Source candidates for position",
                "expected_output": "List of 20+ qualified candidates"
            },
            "relations": {}
        },
        {
            "id": "screening task",
            "vendorClass": "Task",
            "mapsTo": "agento:Agent",
            "attributes": {
                "description": "Analyze and rank candidates",
                "expected_output": "Ranked candidate list with scores",
                "output_pydantic": "CandidateRanking",
                "context": "sourcing_task"
            },
            "relations": {}
        },
        {
            "id": "scheduling task",
            "vendorClass": "Task",
            "mapsTo": "agento:Agent",
            "attributes": {
                "description": "Schedule interviews with top candidates",
                "expected_output": "Interview calendar with confirmed slots",
                "context": "screening_task"
            },
            "relations": {}
        }
    ],
