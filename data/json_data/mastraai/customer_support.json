{
    "framework": "MastraAI",
    "file_name": "customer_support.yaml",
    "pattern_type": "Multi-agentconfiguration+workflowdescription",
    "description": "Pattern ini mendefinisikan sistem bernama “Customer Support System” yang terdiri dari dua agent (support_agent, escalation_agent) dan sebuah workflow bernama support_flow. Setiap agent memiliki role, instructions, dan model. Workflow hanya didefinisikan dengan name dan description tanpa rincian langkah",
    "entities": [
        {
            "id": "system",
            "vendorClass": "name",
            "mapsTo": "agento:Agent",
            "attributes": {
                "name": "Customer Support System"
            },
            "relations": {}
        },
        {
            "id": "#1",
            "vendorClass": "agents[0]",
            "mapsTo": "agento:Agent",
            "attributes": {
                "name": "support_agent",
                "role": "Support Agent",
                "instructions": "Handle customer inquiries",
                "model": "gpt-4"
            },
            "relations": {}
        },
        {
            "id": "#2",
            "vendorClass": "agents[1]",
            "mapsTo": "agento:Agent",
            "attributes": {
                "name": "escalation_agent",
                "role": "Escalation Specialist",
                "instructions": "Handle complex issues",
                "model": "gpt-4"
            },
            "relations": {}
        },
        {
            "id": "workflow #1",
            "vendorClass": "workflows[0]",
            "mapsTo": "agento:Agent",
            "attributes": {
                "name": "support_flow",
                "description": "Customer support workflow"
            },
            "relations": {}
        }
    ],
    "ontologyRelationalProperties": [
        {
            "name": "hasAgent",
            "domain": "agento:MultiAgentSystem",
            "range": "agento:Agent",
            "definition": "Sistem berisi agent-agent tertentu.",
            "status_in_pattern": "ditambahkan"
        },
        {
            "name": "configuredBy",
            "domain": "agento:Agent",
            "range": "agento:LLMModel",
            "definition": "Agent dikonfigurasi menggunakan model tertentu.",
            "status_in_pattern": "ditambahkan"
        },
        {
            "name": "hasWorkflow",
            "domain": "agento:MultiAgentSystem",
            "range": "agento:Workflow",
            "definition": "Sistem memiliki satu atau lebih workflow.",
            "status_in_pattern": "ditambahkan"
        }
    ],
    "newOntologyTerms": {
        "newClasses": [
            {
                "name": "agento:MultiAgentSystem",
                "definition": "Sistem yang mengelompokkan beberapa agent & workflow.",
                "status_in_pattern": "used"
            },
            {
                "name": "agento:LLMModel",
                "definition": "Representasi model (gpt-4, dll).",
                "status_in_pattern": "used"
            },
            {
                "name": "agento:Workflow",
                "definition": "Alur kerja tingkat tinggi.",
                "status_in_pattern": "used"
            }
        ],
        "datatypeProperties": [
            {
                "name": "role",
                "domain": "agento:Agent",
                "range": "xsd:string",
                "justification": "\"Support Agent\", \"Escalation Specialist\"."
            },
            {
                "name": "instructions",
                "domain": "agento:Agent",
                "range": "xsd:string",
                "justification": "\"Handle customer inquiries\"."
            },
            {
                "name": "modelName",
                "domain": "agento:Agent",
                "range": "xsd:string",
                "justification": "\"gpt-4\"."
            },
            {
                "name": "workflowDescription",
                "domain": "agento:Agent",
                "range": "xsd:string",
                "justification": "\"Customer support workflow\"."
            }
        ],
        "optionalProperties": []
    }
}