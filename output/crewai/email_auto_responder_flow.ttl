@prefix : <http://mapping.example.com/> .
@prefix agento: <http://example.org/agento#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix ex: <http://www.ex.org/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#> .
@prefix ontology: <http://data.sfgov.org/ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rml: <http://w3id.org/rml/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.ex.org/Agent/email%20classifier> a agento:Agent;
  agento:vendorClass "Agent" .

<http://www.ex.org/Agent/email%20monitoring%20task> a agento:Agent;
  agento:vendorClass "Task" .

<http://www.ex.org/Agent/emailresponderflow> a agento:Agent;
  agento:vendorClass "Flow" .

<http://www.ex.org/Agent/human%20reviewer> a agento:Agent;
  agento:vendorClass "Agent" .

<http://www.ex.org/Agent/response%20generator> a agento:Agent;
  agento:vendorClass "Agent" .

<http://www.ex.org/DatatypeProperty/emailCategory> a agento:DatatypeProperty;
  agento:domain agento:Agent;
  agento:justification "Email classification adalah core functionality pattern ini.";
  agento:name "emailCategory";
  agento:range xsd:string .

<http://www.ex.org/DatatypeProperty/monitoringInterval> a agento:DatatypeProperty;
  agento:domain agento:Agent;
  agento:justification "Continuous monitoring requires interval configuration.";
  agento:name "monitoringInterval";
  agento:range xsd:string .

<http://www.ex.org/NewClass/agento%3AHumanInputAgent> a agento:NewClass;
  agento:definition "Agent yang membutuhkan human intervention.";
  agento:name "agento:HumanInputAgent";
  agento:statusInPattern "used" .

<http://www.ex.org/OntologyRelProp/classifiesAs> a agento:OntologyRelationalProperty;
  agento:definition "Email classification results.";
  agento:domain agento:Task;
  agento:name "classifiesAs";
  agento:range agento:Category;
  agento:statusInPattern "digunakan" .

<http://www.ex.org/OntologyRelProp/monitorsSource> a agento:OntologyRelationalProperty;
  agento:definition "Flow monitors external data source.";
  agento:domain agento:Flow;
  agento:name "monitorsSource";
  agento:range agento:DataSource;
  agento:statusInPattern "digunakan" .

<http://www.ex.org/OntologyRelProp/requiresApproval> a agento:OntologyRelationalProperty;
  agento:definition "Response membutuhkan human approval.";
  agento:domain agento:Agent;
  agento:name "requiresApproval";
  agento:range agento:Agent;
  agento:statusInPattern "digunakan" .

