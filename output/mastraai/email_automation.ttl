@prefix : <http://mapping.example.com/> .
@prefix agento: <http://example.org/agento#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix ex: <http://www.ex.org/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#> .
@prefix ontology: <http://data.sfgov.org/ontology/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rml: <http://w3id.org/rml/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.ex.org/Agent/%231> a agento:Agent;
  agento:name "email_classifier";
  agento:vendorClass "agents[0]" .

<http://www.ex.org/Agent/%232> a agento:Agent;
  agento:name "responder";
  agento:vendorClass "agents[1]" .

<http://www.ex.org/Agent/system%20name> a agento:Agent;
  agento:name "Email Automation";
  agento:vendorClass "name" .

<http://www.ex.org/Agent/workflow%20%231> a agento:Agent;
  agento:name "email_handling";
  agento:vendorClass "workflows[0]" .

<http://www.ex.org/DatatypeProperty/description> a agento:DatatypeProperty;
  agento:domain agento:Agent;
  agento:justification "Field description pada workflows[0] menjelaskan alur “Classify and respond to emails”.";
  agento:name "description";
  agento:range xsd:string .

<http://www.ex.org/DatatypeProperty/instructions> a agento:DatatypeProperty;
  agento:domain agento:Agent;
  agento:justification "Field instructions menyimpan deskripsi natural-language untuk operasi masing-masing agent.";
  agento:name "instructions";
  agento:range xsd:string .

<http://www.ex.org/DatatypeProperty/modelName> a agento:DatatypeProperty;
  agento:domain agento:Agent;
  agento:justification "Nilai \"gpt-4\" mengindikasikan model LLM yang digunakan.";
  agento:name "modelName";
  agento:range xsd:string .

<http://www.ex.org/DatatypeProperty/name> a agento:DatatypeProperty;
  agento:domain agento:Agent;
  agento:justification "Field name muncul pada root (\"Email Automation\"), pada agent, dan pada workflow.";
  agento:name "name";
  agento:range xsd:string .

<http://www.ex.org/DatatypeProperty/role> a agento:DatatypeProperty;
  agento:domain agento:Agent;
  agento:justification "Field role membedakan fungsi Email Classifier dan Auto Responder.";
  agento:name "role";
  agento:range xsd:string .

<http://www.ex.org/NewClass/agento%3ALLMModel> a agento:NewClass;
  agento:definition "Representasi model LLM yang dikonfigurasi untuk agent.";
  agento:name "agento:LLMModel";
  agento:statusInPattern "used" .

<http://www.ex.org/NewClass/agento%3AMultiAgentSystem> a agento:NewClass;
  agento:definition "Representasi top-level sistem yang menaungi beberapa agent dan workflow.";
  agento:name "agento:MultiAgentSystem";
  agento:statusInPattern "used" .

<http://www.ex.org/NewClass/agento%3AWorkflow> a agento:NewClass;
  agento:definition "Representasi alur kerja konseptual dalam sistem.";
  agento:name "agento:Workflow";
  agento:statusInPattern "used" .

<http://www.ex.org/OntologyRelProp/configuredBy> a agento:OntologyRelationalProperty;
  agento:definition "Menyatakan bahwa sebuah agent dikonfigurasi untuk menggunakan model LLM tertentu.";
  agento:domain agento:Agent;
  agento:name "configuredBy";
  agento:range agento:LLMModel;
  agento:statusInPattern "ditambahkan / digunakan kembali di agento." .

<http://www.ex.org/OntologyRelProp/hasAgent> a agento:OntologyRelationalProperty;
  agento:definition "Menyatakan bahwa suatu sistem multi-agent memiliki sekumpulan agent tertentu.";
  agento:domain agento:MultiAgentSystem;
  agento:name "hasAgent";
  agento:range agento:Agent;
  agento:statusInPattern "ditambahkan / digunakan kembali di agento." .

<http://www.ex.org/OntologyRelProp/hasWorkflow> a agento:OntologyRelationalProperty;
  agento:definition "Menyatakan bahwa sistem tersebut mencakup satu atau lebih workflow yang menggambarkan alur kerja.";
  agento:domain agento:MultiAgentSystem;
  agento:name "hasWorkflow";
  agento:range agento:Workflow;
  agento:statusInPattern "ditambahkan (khusus pola mastraai dengan workflow)." .

